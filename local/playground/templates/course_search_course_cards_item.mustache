<li id="coursecarditem-{{{id}}}" class="course-card-item" data-name="{{{name}}}" data-date="{{{sortdate}}}">
    <a href="{{{baseurl}}}{{{link}}}{{{id}}}">
        {{#img}}
            <div class="course-img">
                <img src="{{{baseurl}}}{{{.}}}" alt="" class="img-responsive">
            </div>
        {{/img}}
        <div class="course-desc">
            <h4 class="course-title">{{{name}}}</h4>
            <div class="course-summary">{{{summary}}}</div>
        </div>
    </a>
</li>
